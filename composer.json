  },
  "require-dev": {
    "phpunit/phpunit": "^9.1",
    "phpstan/phpstan": "^1.10",
    "friendsofphp/php-cs-fixer": "^3.54"
  },
  "require": {
    "php": ">=8.1",
    "psr/container": "^2.0"
  }
}
